  location <%= @location %> {
    proxy_pass <%= @proxy %>;
    proxy_read_timeout <%= @proxy_read_timeout %>;
<% unless @proxy_redirect.nil? || @proxy_redirect.empty? -%>
    proxy_redirect <%= @proxy_redirect %>;
<% end -%>
<%  @proxy_set_header.each do |header| -%>
    proxy_set_header        <%= header %>;
<%  end %>
  }

